{
  "name": "RN_App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-run-debug-dev": "react-native run-ios --scheme RN_App-Dev --mode Debug",
    "ios-run-debug-staging": "react-native run-ios --scheme RN_App-Stag --configuration Debug",
    "ios-run-debug-production": "react-native run-ios --scheme RN_App --configuration Debug",
    "android-run-debug-dev": "react-native run-android --mode=devDebug --appIdSuffix=dev",
    "android-run-debug-staging": "react-native run-android --mode=stagDebug --appIdSuffix=stag",
    "android-run-debug-product": "react-native run-android --mode=prodDebug --appIdSuffix=prod",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky",
    "generateimages": "node scripts/genimg src/assets/images --output src/themes/images.ts --name Images"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.0",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@reduxjs/toolkit": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "axios": "^1.6.7",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
